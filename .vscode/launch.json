{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug: Nest API",
        "runtimeArgs": ["start", "--debug", "--watch"],
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nest",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
        "outputCapture": "std",
        "console": "integratedTerminal",
        "protocol": "inspector"
      },
    ]
  }
  